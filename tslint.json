{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "arrow-parens": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "ordered-imports": false
  },
  "rulesDirectory": []
}
